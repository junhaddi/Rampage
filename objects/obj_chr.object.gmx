draw_sprite_ext(spr_chr_gun, chr_class, x + lengthdir_x(5, aim_dir), y + lengthdir_y(5, aim_dir), 1, instance_xdir, aim_dir, c_white, image_alpha);

/// Draw Item Time
if (alarm[2] != -1)
{
    draw_rectangle_color(x - 24, y - 32, x - 24 + (48 / item_time * alarm[2]), y - 24, c_orange, c_orange, c_yellow, c_yellow, 0)

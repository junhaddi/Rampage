if (item_index = 1)
{
    draw_sprite_ext(spr_chr_gun, chr_class, x + lengthdir_x(5, aim_dir), y + lengthdir_y(5, aim_dir) - 8, 1, instance_xdir, aim_dir, c_white, image_alpha);

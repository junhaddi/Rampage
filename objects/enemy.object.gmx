/// Enemy xDirection
if (direction &gt;= 0 &amp;&amp; direction &lt; 90 || direction &gt; 270 &amp;&amp; direction &lt;= 360)
{
    /// 89 ~ 271
    instance_xdir = 1;
}
else if (direction &gt; 90 &amp;&amp; direction &lt; 270)
{
    /// 91 ~ 269
    instance_xdir = -1;
}
else
{
    /// 90, 270
    if (x &lt; obj_chr.x)
    {
        instance_xdir = 1;
    }
    else
    {
        instance_xdir = -1;
    }
}


/// Die!
if (enemy_hp &lt; 1)
